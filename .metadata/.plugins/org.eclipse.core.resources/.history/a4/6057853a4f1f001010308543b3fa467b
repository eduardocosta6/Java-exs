package ficha10;

public class Ex5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int tamanho = 4;
		 for(int i = 1; i <= tamanho; i++) {
			 for ( int c = 1; c <= tamanho; c++) {
				 if (i == c ) {
					 System.out.print("1");
				 }
			 }
			 System.out.print("\n");
		 }

	}

}

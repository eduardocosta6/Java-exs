package ficha10;

public class Ex4A {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		 int n = 10;
		 for(int i = 1; i <= n; i++) {
			 for ( int c = 1; c <= n; c++) {
				 if (c == n - 1) {
					 System.out.print("1");
				 } else {
					 System.out.print("0");
				 }
			 }
			 System.out.print("\n");
		 }


	}

}

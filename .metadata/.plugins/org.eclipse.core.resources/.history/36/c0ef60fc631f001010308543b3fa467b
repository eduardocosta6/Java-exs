package ficha10;

public class Ex1D {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int tamanho = 5;
		 for(int l = 1; l <= tamanho; l++) {
			 for ( int c = 1; c <= tamanho; c++) {
				 if ( c == l || c == tamanho - l ) {
					 System.out.print(l);
				 } else {
					 System.out.print(" ");
				 }
			 }
			 System.out.println();
			 
		 }
		 
	}
}

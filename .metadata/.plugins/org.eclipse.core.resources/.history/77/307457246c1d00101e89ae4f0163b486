package ficha10;

public class Ex4A {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		 int tamanho = 5;
		 for(int l = 1; l <= tamanho; l++) {
			 for ( int c = 1; c <= tamanho; c++) {
				 if ( c <= l) {
					 System.out.print(l + " " );
				 } 
				 
				 int tamanho2 = 5;
				 for(int l2 = 1; l <= tamanho; l2++) {
					 for ( int c2 = 1; c2 <= tamanho; c2++) {
						 if (  c2 <= l2) {
							 System.out.print(l - c2 +1 );
						 }
					 }
					 System.out.print("\n");
				 }
					 
				 }
			 }
			 System.out.print("\n");
		 }


	}

}




package ficha10;

public class Ex4A {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int tamanho = 10;
		int alt = 5;
		 for(int l = 1; l <= tamanho; l++) {
			 for ( int c = 1; c <= 10; c++) {
				 if (  c <= l) {
					 System.out.print(  c - l + tamanho + " ");
				 }
			 }
			 System.out.print("\n");
		 }


	}

}
